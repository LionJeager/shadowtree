<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shadow.summer.Dao.UserDao">
    <select id="AuthLogin" parameterType="java.util.Map" resultType="java.lang.Integer">
        select count(*) from login_user
        <where>
           user = #{phoneNum} and password = #{password}
        </where>
    </select>

    <insert id="addOne" parameterType="java.util.Map">
        insert into login_user(user,password,role,token,gender,job) values(#{phoneNum},#{password},'C','XX2','1','None')
    </insert>

</mapper>